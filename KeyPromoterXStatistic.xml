<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Actual Size">
        <Item>
          <IdeaActionID>Images.Editor.ActualSize</IdeaActionID>
          <ShortCut>'⌘NumPad /' or '⌘/'</ShortCut>
          <Description>Actual Size</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Message History">
        <Item>
          <IdeaActionID>Vcs.ShowMessageHistory</IdeaActionID>
          <ShortCut>'⌃M' or '⌘E'</ShortCut>
          <Description>Commit Message History</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'⇧⏎'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Disconnect">
        <Item>
          <IdeaActionID>DatabaseView.CloseConnectionAction</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Disconnect</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'⇧⌘F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'start'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'start'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>DatabaseView.SynchronizeAction</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Zoom In">
        <Item>
          <IdeaActionID>Images.Editor.ZoomIn</IdeaActionID>
          <ShortCut>'⌘+' or '⌘='</ShortCut>
          <Description>Zoom In</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Zoom Out">
        <Item>
          <IdeaActionID>Images.Editor.ZoomOut</IdeaActionID>
          <ShortCut>'⌘-' or '⌘-'</ShortCut>
          <Description>Zoom Out</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>